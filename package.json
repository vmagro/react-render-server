{
  "name": "render-server",
  "version": "0.0.1",
  "description": "Lightweight node server to compile and render a React application",
  "main": "main.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/babel-cli/bin/babel-node.js lib/server.js",
    "clean": "rm -rf build && mkdir build",
    "babel": "./node_modules/babel-cli/bin/babel.js -d ./build ./lib -s",
    "build": "npm run clean && npm run babel",
    "babel-example": "./node_modules/babel-cli/bin/babel.js -d ./build/example ./example -s",
    "prod": "NODE_ENV=production node ./build/server.js",
    "mocha": "./node_modules/mocha/bin/mocha --require babel-register lib/**/**.spec.js",
    "test": "npm run mocha && npm run lint",
    "lint": "./node_modules/eslint/bin/eslint.js *.js lib/*.js"
  },
  "author": "Vinnie Magro <v@vinnie.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "chai": "^4.0.2",
    "connect-browser-sync": "^2.1.0",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "gulp": "^3.9.1",
    "mocha": "^3.4.2",
    "mock-require": "^2.0.2",
    "nock": "^9.0.13",
    "require-reload": "^0.2.2",
    "supertest": "^3.0.0",
    "webpack": "^3.1.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router": "^4.1.1",
    "winston": "^2.3.1"
  }
}
